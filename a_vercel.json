{
  "version": 2,
  "name": "park-screen",
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist/index.html" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
