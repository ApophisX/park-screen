{
  "version": 2,
  "name": "park-screen",
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "handle": "filesystem"
    },

    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "http://sunkoda.us.to:9002/$1"
    }
  ]
}
